dsl Ness

use ExampleEvents

Broker(path = empty, port = empty, secondaryPort = empty)
	User("example", "example")

Datalake(scale = Month, path = empty) dl

	Tank createHospital as Event(example.CreateHospital)
	Tank createVaccine as Event(example.CreateVaccine)
	Tank vaccine as Event(example.Vaccine)

Terminal exampleTerminal
	Publish (dl.createHospital dl.createVaccine dl.vaccine)
	Subscribe (dl.createHospital dl.createVaccine dl.vaccine)